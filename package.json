{
  "name": "voice-studio-app",
  "productName": "VoiceStudio",
  "version": "1.0.0",
  "description": "Voice Studio - AI Voice Generation Tool",
  "main": ".webpack/main",
  "scripts": {
    "start": "node src/main/installers/setup.js && env-cmd -f .env electron-forge start",
    "package": "env-cmd -f .env electron-forge package",
    "make": "env-cmd -f .env electron-forge make",
    "make:mac": "env-cmd -f .env electron-forge make --platform=darwin --arch=arm64",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "setup": "node src/main/installers/setup.js"
  },
  "keywords": [],
  "author": "Kanishka",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@electron-forge/cli": "^7.7.0",
    "@electron-forge/maker-deb": "^7.7.0",
    "@electron-forge/maker-dmg": "^7.7.0",
    "@electron-forge/maker-rpm": "^7.7.0",
    "@electron-forge/maker-squirrel": "^7.7.0",
    "@electron-forge/maker-zip": "^7.7.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.7.0",
    "@electron-forge/plugin-fuses": "^7.7.0",
    "@electron-forge/plugin-webpack": "^7.7.0",
    "@electron/fuses": "^1.8.0",
    "@electron/osx-sign": "^1.3.3",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.11.0",
    "electron": "34.2.0",
    "electron-notarize": "^1.2.2",
    "fs-extra": "^11.2.0",
    "node-loader": "^2.1.0",
    "shelljs": "^0.8.5",
    "style-loader": "^3.3.4"
  },
  "dependencies": {
    "dockerode": "^4.0.4",
    "dotenv": "^16.4.7",
    "electron-squirrel-startup": "^1.0.1",
    "node-fetch": "^2.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "systeminformation": "^5.25.11",
    "ws": "^8.18.1"
  }
}
